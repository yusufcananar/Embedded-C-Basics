#include <stdint.h>
#include <stdio.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

int main(void)
{
	__asm volatile(
			"LDR R1, =#0x20001000"
			"LDR R2, =#0x20001004"
			"LDR R0,[R1]"
			"LDR R1,[R2]"
			"ADD R0, R0, R1"
			"STR R0, [R2]"
			);

	printf("Hello world\n");
    /* Loop forever */
	for(;;);
}
